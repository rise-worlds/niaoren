<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <stroke android:width="1dp" android:color="#6553db"/>
            <solid android:color="#a395ff"/>
            <corners android:radius="@dimen/fz"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <solid android:color="@color/as"/>
                <corners android:radius="@dimen/fz"/>
            </shape>
        </clip>
    </item>
</layer-list>
